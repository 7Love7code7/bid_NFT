// Mutates the NFT ID on every transfer, something which may or may not be possible against the ERC721 spec
// It's not explicitly impossible, with the only sticking point being the Transfer event
// It is a bad idea, yet the hook on receiving NFTs does theoretically enable such behavior
contract RemintingNFT is ERC721 {
  constructor (string memory name_, string memory symbol_) ERC721(name_, symbol_) {}

  function _transfer(address from, address to, uint256 tokenId) internal override {
    require(ERC721.ownerOf(tokenId) == from, "ERC721: transfer of token that is not own"); // internal owner
    require(to != address(0), "ERC721: transfer to the zero address");

    _burn(tokenId);
    _mintActual(to, tokenId + 1);
    emit Transfer(from, to, tokenId);
  }

  function _safeTransfer(address from, address to, uint256 tokenId, bytes memory _data) internal override {
      _transfer(from, to, tokenId);
      require(_checkOnERC721Received(from, to, tokenId + 1, _data), "ERC721: transfer to non ERC721Receiver implementer");
  }
}
